from cobra.io import read_sbml_model
model = read_sbml_model('iNF517.xml')
%%time
copy_of_model = model.copy()
%%time
for gene in model.genes:
    mutant = model.copy()
    mutant.genes.get_by_id(gene.id).knock_out()
    %%time
with model:
    for gene in model.genes:
        gene.knock_out()
